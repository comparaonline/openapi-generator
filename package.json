{
    "name": "@comparaonline/openapi-generator",
    "version": "1.0.2",
    "description": "Soap and rest connector",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "repository": "https://github.com/comparaonline/openapi-generator.git",
    "engines": {
        "node": ">=16.20.0"
    },
    "scripts": {
        "build": "tsc",
        "lint": "ts-standard src --fix",
        "test": "jest",
        "pretest": "yarn lint",
        "tester": "ts-node src/tester.ts",
        "check-code": "ts-prune ./src | grep -v 'used in module'"
    },
    "author": {
        "name": "Mat√≠as Theiler",
        "email": "mtheiler@comparaonline.com"
    },
    "license": "MIT",
    "private": false,
    "module": "true",
    "files": [
        "build"
    ],
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "comparaonline",
        "express",
        "joi",
        "openapi",
        "swagger"
    ],
    "dependencies": {
        "joi": "^17.10.2",
        "joi-to-swagger": "^6.2.0",
        "swagger-ui-express": "^5.0.0",
        "ts-json-schema-generator": "^1.3.0",
        "http-status-codes": "^2.3.0"
    },
    "peerDependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.18",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.15",
        "@types/supertest": "^2.0.14",
        "@types/swagger-ui-express": "^4.1.4",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.3.1",
        "prettier": "^2.4.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.4.0",
        "ts-prune": "^0.10.3",
        "ts-standard": "^12.0.2",
        "typescript": "^4.9.4"
    }
}